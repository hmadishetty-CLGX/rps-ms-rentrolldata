<?xml version="1.0"?>
<FindBugsFilter>

    <!-- Suppress outlined warnings for tests classes -->
    <Match>
        <Class name="~.*\.*Test"/>
        <Bug pattern="
            NP_NONNULL_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR,
            UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
</FindBugsFilter>